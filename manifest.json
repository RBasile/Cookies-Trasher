{
  "browser_specific_settings": {
  "gecko": {
    "id": "cookies.trash@rbwork.fr"
  }
},
  "browser_action": {
    "browser_style": true,
    "default_title": "Trash Cookies in the active tab",
    "default_popup": "trash.html",
    "default_icon": {
      "19": "icons/default19.png",
      "38": "icons/default38.png"
    }
  },
  "description": "Trash Cookies in the active tab",
  "icons": {
    "48": "icons/default48.png",
    "96": "icons/default96.png"
  },
  "homepage_url": "https://rbwork.fr",
  "manifest_version": 2,
  "name": "Cookies Trasher",
  "version": "2.0",
  "permissions": ["cookies","<all_urls>","tabs"]
}
